{
  "name": "authoredup-app",
  "version": "2.2.9",
  "description": "AuthoredUp app and extension",
  "private": true,
  "scripts": {
    "web-start": "next start",
    "web-dev": "next dev",
    "web-build": "next build && npm run web-sitemap",
    "web-sitemap": "next-sitemap",
    "ext-watch": "support/extension-watch.js",
    "ext-build-dev": "support/extension-build.js --dev --include-revision --zip --store",
    "ext-build-stag": "support/extension-build.js --target staging --zip --include-revision --store",
    "ext-build-insider": "support/extension-build.js --target insider --zip --include-revision --store",
    "ext-build-live": "support/extension-build.js --target live --zip --verify-repo --store",
    "ext-publish-insider": "support/extension-publish.js --target insider",
    "ext-analyze-dev": "support/extension-build.js --dev --stats && webpack-bundle-analyzer dist/dev/stats.json --port 9999",
    "ext-analyze-live": "support/extension-build.js --target live --stats && webpack-bundle-analyzer dist/prod/stats.json --port 9999",
    "harness-dev": "support/harness-dev.js",
    "harness-build": "support/harness-build.js",
    "ext-clean": "rm -rf dist",
    "test": "jest",
    "lint": "next lint",
    "gen-css": "node support/generate-css.js",
    "gen-examples": "node support/generate-examples.js",
    "gen-backends": "node support/generate-backends.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc --noEmit",
    "lintcheck": "next lint --quiet",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
